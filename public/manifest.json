{
  "manifest_version": 3,
  "name": "Suwat - by yourtechy.com",
  "description": "suwat - The speech transcriber by yourtechy.com",
  "key": "-----BEGINPUBLICKEY----------ENDPUBLICKEY-----",
  "oauth2": {
    "client_id": "466910947435-k8868vvhpktebb3v008p4ue9kg7ubqg3.apps.googleusercontent.com",
    "project_id": "digital-seat-130823",
    "auth_uri": "https://accounts.google.com/o/oauth2/auth",
    "token_uri": "https://oauth2.googleapis.com/token",
    "auth_provider_x509_cert_url": "https://www.googleapis.com/oauth2/v1/certs",
    "scopes": ["https://www.googleapis.com/auth/userinfo.email"]
  },
  "icons": {
    "128": "icon-34.png"
  },
  "permissions": [
    "cookies",
    "webNavigation",
    "storage",
    "unlimitedStorage",
    "contextMenus",
    "sidePanel",
    "tabs",
    "activeTab",
    "tabCapture",
    "offscreen",
    "identity",
    "notifications"
  ],
  "host_permissions": [
    "*://suwat.com/*"
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "styles.css",
        "icon-128.png", 
        "icon-34.png", 
        "*.html"
      ],
      "matches": []
    }
  ],
  "action": {
    "default_panel": "sidepanel.html",
    "default_title": "suwat",
    "default_icon": "icon-34.png"
  },
  "background": {
    "service_worker": "background.bundle.js"
  },
  "devtools_page": "devtools.html",
  "options_page": "options.html"
}
